#include <stdio.h>

int main() {
  long x, cx;
  printf("–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ\n");
  scanf("%ld", &x);
  while (x < 100 || x > 1000000000) {
    printf("–í–≤–µ–¥–∏—Ç–µ –∫–æ—Ä—Ä–µ–∫—Ç–Ω–æ–µ —á–∏—Å–ª–æ\n");
    scanf("%ld", &x); 
  }
  cx = x; x = 0;
  long by3 = 0, rem1by3 = 0, rem2by3 = 0;
  
  while (cx > 0) { x = x * 10 + cx % 10; cx /= 10; }
  
  while (x > 0) {
    int rem = x % 10;
    x /= 10;
    if (rem % 3 == 0) {
      by3 = by3 * 10 + rem;
    }
    else if (rem % 3 == 1) {
      rem1by3 = rem1by3 * 10 + rem;
    }
    else if (rem % 3 == 2) {
      rem2by3 = rem2by3 * 10 + rem;
    }
  }
  
  long sum = by3 + rem1by3 + rem2by3;
  printf("%ld, %ld, %ld, %ld\n", by3, rem1by3, rem2by3, sum);
  
  return 0;
}
